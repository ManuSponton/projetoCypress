{
    "name": "projeto-z",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "lint": "eslint cypress/support/*.js --color",
        "lint:fix": "eslint --fix cypress/support/*.js",
        "test:e2e:ci": "cypress run --env NODE_ENV=ci",
        "test:e2e:local": "cypress run --env NODE_ENV=local",
        "test:e2e:ci:headed": "cypress run --env NODE_ENV=ci --headed",
        "cypress:open": "cypress open --env NODE_ENV=ci",
        "e2e:ci:mobile": "cypress run --config viewportWidth=375,viewportHeight=812  --env NODE_ENV=ci",
        "e2e:ci:tablet": "cypress run --config viewportWidth=768,viewportHeight=800  --env NODE_ENV=ci",
        "e2e:ci:desktop": "cypress run --config viewportWidth=1280,viewportHeight=720  --env NODE_ENV=ci",
        "e2e:mobile": "cypress open --config viewportWidth=375,viewportHeight=812  --env NODE_ENV=ci",
        "e2e:tablet": "cypress open --config viewportWidth=768,viewportHeight=800  --env NODE_ENV=ci",
        "e2e:desktop": "cypress open --config viewportWidth=1280,viewportHeight=720  --env NODE_ENV=ci"
    },
    "devDependencies": {
        "cypress": "4.5.0",
        "cypress-cucumber-preprocessor": "1.16.0",
        "cypress-failed-log": "2.5.1",
        "cypress-promise": "^1.0.2",
        "cypress-xpath": "1.3.0",
        "eslint": "5.15.3",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-config-prettier": "^4.3.0",
        "eslint-config-standard": "12.0.0",
        "eslint-plugin-import": "2.16.0",
        "eslint-plugin-node": "8.0.1",
        "eslint-plugin-prettier": "3.0.1",
        "eslint-plugin-promise": "4.0.1",
        "eslint-plugin-standard": "4.0.0",
        "jest-junit": "6.4.0",
        "prettier": "1.16.4"
    }
}
